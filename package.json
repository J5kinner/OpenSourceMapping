{
  "name": "@faims-project/faims3-map-input",
  "version": "1.0.6",
  "description": "Map based input widget for FAIMS3",
  "author": "stevecassidy",
  "license": "MIT",
  "repository": "FAIMS/faims3-map-input",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "module": "build/index.modern.js",
  "source": "./src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rm -rf build/*; microbundle --jsx React.createElement --no-compress --format modern,cjs",
    "start": "microbundle watch --jsx React.createElement --no-compress --format modern,cjs",
    "rollup": "rollup -c",
    "test": "cross-env CI=1 react-scripts test --env=jsdom --transformIgnorePatterns \"node_modules/(?!ol)/\"",
    "test:watch": "react-scripts test --env=jsdom --transformIgnorePatterns \"node_modules/(?!ol)/\"",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "prepare": "yarn run build",
    "pretest": "yarn run build",
    "posttest": "yarn run lint"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gh-pages": "^3.2.3",
    "gts": "^3.1.0",
    "microbundle": "^0.14.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "typescript": "^4.3.5"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@capacitor/core": "3.5.1",
    "@capacitor/geolocation": "^1.3.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "5.8.1",
    "@testing-library/dom": ">=5",
    "eslint-plugin-flowtype": "^6.1.1",
    "formik": "^2.2.9",
    "ol": "^6.8.1"
  }
}
